<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Appointment.Views.EmployeeAppointmentMain">
  <StackLayout VerticalOptions="Start">
    <Label Text="{Binding EmpName}" VerticalOptions="Center" HorizontalOptions="Center" FontSize="20" />
    <Label Text="{Binding HasAppointment}" VerticalOptions="Center" HorizontalOptions="Center" FontSize="20" />
    <Button Text="Back"  Command="{Binding goBackCommand}"/>
  </StackLayout>
</ContentPage>